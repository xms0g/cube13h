all:
	MAKE.EXE clean
	TASM.EXE /ml MEM.ASM 
	TCC.EXE -O -c -oVGA.OBJ VGA.C
	TCC.EXE -O -c -oRENDERER.OBJ RENDERER.C
	TCC.EXE -O -c -oKEYBRD.OBJ KEYBRD.C
	TCC.EXE -O -c -oVEC.OBJ VEC.C
	TCC.EXE -O -c -oMAT.OBJ MAT.C
	TCC.EXE -O -c -oMATH.OBJ MATH.C
	TCC.EXE -O -c -oTRINGL.OBJ TRINGL.C
	TCC.EXE -O CUBE13H.C MEM.OBJ VGA.OBJ RENDERER.OBJ KEYBRD.OBJ VEC.OBJ MAT.OBJ MATH.OBJ TRINGL.OBJ
clean:
	DEL CUBE13H.EXE
	DEL CUBE13H.OBJ
	DEL MEM.OBJ
	DEL VGA.OBJ
	DEL RENDERER.OBJ
	DEL KEYBRD.OBJ
	DEL VEC.OBJ
	DEL MAT.OBJ
	DEL MATH.OBJ
	DEL TRINGL.OBJ
	